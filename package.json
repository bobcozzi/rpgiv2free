{
  "name": "rpgivfree",
  "displayName": "Bob Cozzi's IBM i RPG IV to RPG Free Conversion Tool",
  "description": "Bob Cozzi's Convert RPG IV fixed-format to free-format tool add-on for Visual Studio CODE for i.",
  "icon": "images/rpgfree.png",
  "version": "0.0.3",
  "publisher": "bobcozzi",
  "license": "SEE LICENSE in license.txt",
  "engines": {
    "vscode": "^1.70.0"
  },
  "keywords": [
    "ibmi",
    "rpgle",
    "rpgiv",
    "iseries",
    "ilerpg",
    "rpgfree",
    "as400"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bobcozzi/vsciRPGConverter.git"
  },
  "categories": [
    "Other"
  ],
   "contributes": {
    "commands": [
      {
        "command": "rpgconverter.convertToRPGFree",
        "title": "Convert RPG IV to Free Format"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "rpgconverter.convertToRPGFree",
          "when": "resourceExtname =~ /rpg(le)?|sqlrpgle/i",
          "group": "navigation"
        }
      ]
    }
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:rpgconverter.convertToRPGFree"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "build": "tsc",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "typescript": "^4.8.0"
  },
  "dependencies": {
    "sql-formatter": "^15.5.2"
  }
}