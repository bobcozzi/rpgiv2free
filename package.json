{
  "name": "rpg-converter",
  "displayName": "RPG Fixed to Free Converter",
  "description": "Converts RPG fixed-format source to free-format inline in the editor.",
  "version": "0.0.1",
  "publisher": "Bob Cozzi",
  "engines": {
    "vscode": "^1.70.0"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/bobcozzi/vsciRPGConverter.git"
},
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:rpgConverter.convert"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rpgConverter.convert",
        "title": "Convert RPG Fixed to Free Format"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "typescript": "^4.8.0",
    "vsce": "^1.97.0"
  },
  "dependencies": {
    "-": "^0.0.1",
    "g": "^2.0.1"
  }
}
